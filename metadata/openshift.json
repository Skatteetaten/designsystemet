{
  "docker": {
    "maintainer": "Aurora OpenShift Utvikling <utvpaas@skatteetaten.no>",
    "labels": {
      "io.k8s.description": "Skatteetatens designsystem",
      "io.openshift.tags": "openshift,react,nodejs"
    }
  },
  "web": {
    "webapp": {
      "content": "dist/storybook/ds-playground",
      "path": "/web/designsystemet/",
      "headers": {
        "Cache-Control": "no-cache"
      },
      "locations": {
        "componentIcons/*.svg": {
          "headers": {
            "Cache-Control": "no-cache"
          }
        },
        "playroom/index.html": {
          "headers": {
            "Cache-Control": "no-cache"
          }
        }
      }
    }
  }
}
